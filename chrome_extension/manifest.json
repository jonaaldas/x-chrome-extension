{
    "name": "Twitter publish ASAP",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Publishes a tweet as soon as possible",
    "permissions": ["commands"],
    "action": {
        "default_icon": "tweet.png",
        "default_popup": "popup.html",
        "default_title": "Click to view a popup"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+U",
                "mac": "Command+U"
            }
        }
    }
}
